  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BloggifyTutorials/todos-react.git"
  },
  "homepage": "https://github.com/BloggifyTutorials/todos-react#readme",
  "blah": {
    "title": "TODOs",
    "installation_command": {
      "content": [
        "# Clone this repository",
        "git clone https://github.com/BloggifyTutorials/todos-react",
        "",
        "# Install the dependencies",
        "cd todos-react && npm i",
        "",
        "# Start the app in dev mode",
        "npm run start:dev"
      ],
      "language": "bash"
    },
